    "ribosome": {
        "data": {
            "active_counts": {
                "table": "UniqueMoleculeCounts",
                "column": "uniqueMoleculeCounts",
                "subcolumn": "activeRibosome"
            },
            "s30_counts": {
                "table": "BulkMolecules",
                "column": "counts",
                "subcolumn": "CPLX0-3953[c]"
            },
            "s50_counts": {
                "table": "BulkMolecules",
                "column": "counts",
                "subcolumn": "CPLX0-3962[c]"
            },
           "volume": {
                "table": "Mass",
                "column": "cellVolume"
           }
        },
        "operations": {
            "inactive_counts": {
                "function": "elementwise_min",
                "args": ["s30_counts", "s50_counts"]
            },
            "active_fraction": {
                "function": "calc_active_fraction",
                "args": ["active_counts", "inactive_counts"]
            },
            "total_counts": {
                "function": "add_two_arrays",
                "args": ["inactive_counts", "active_counts"]
            },
            "concentration": {
                "function": "elementwise_divide",
                "args": ["total_counts", "volume"]
            },
            "mean_concentration": {
                "function": "mean",
                "args": ["concentration"],
                "range": [0, 0]
            },
            "mean_active_fraction": {
                "function": "mean",
                "args": ["active_fraction"],
                "range": [0, 0]
            }
        }
    },
    "rnaPolymerase": {
        "data": {
            "active_counts": {
                "table": "UniqueMoleculeCounts",
                "column": "uniqueMoleculeCounts",
                "subcolumn": "activeRnaPoly"
            },
           "inactive_counts": {
                "table": "BulkMolecules",
                "column": "counts",
                "subcolumn": "APORNAP-CPLX[c]"
           },
           "volume": {
                "table": "Mass",
                "column": "cellVolume"
           }
        },
        "operations": {
            "active_fraction": {
                "function": "calc_active_fraction",
                "args": ["active_counts", "inactive_counts"]
            },
            "mean_active_fraction": {
                "function": "mean",
                "args": ["active_fraction"],
                "range": [0, 0]
            },
            "total_counts": {
                "function": "add_two_arrays",
                "args": ["active_counts", "inactive_counts"]
            },
            "concentration": {
                "function": "elementwise_divide",
                "args": ["total_counts", "volume"]
            },
            "mean_concentration": {
                "function": "mean",
                "args": ["concentration"],
                "range": [0, 0]
            }
        }
    },
    "doublingTime": {
        "data": {
            "time": {
                "table": "Main",
                "column": "time"
            },
            "start_time": {
                "table": "Main",
                "attribute": "initialTime"
            }
        },
        "operations": {
            "end_time": {
                "function": "last_elem",
                "args": ["time"]
            },
            "doubling_time": {
                "function": "scalar_subtract",
                "args": ["end_time", "start_time"],
                "range": [0, 0]
            }
        }
    },
    "translationElongationRate": {
        "data": {
            "rate": {
                "table": "RibosomeData",
                "column": "effectiveElongationRate"
            }
        },
        "operations": {
            "mean": {
                "function": "mean",
                "args": ["rate"],
                "range": [0, 0]
            }
        }
    },
    "transcriptionElongationRate": {
        "data": {
            "bases": {
                "table": "TranscriptElongationListener",
                "column": "countNTPsUSed"
            },
            "time": {
                "table": "Main",
                "column": "time"
            },
            "initial_time": {
                "table": "Main",
                "attribute": "initialTime"
            },
            "slice_start": {
                "constant": 1
            },
            "slice_end": {
                "constant": null
            }
        },
        "operations": {
            "time_deltas": {
                "function": "pairwise_diffs",
                "args": ["time"]
            },
            "all_but_first_bases": {
                "function": "slice",
                "args": ["bases", "slice_start", "slice_end"]
            },
            "rate": {
                "function": "elementwise_divide",
                "args": ["all_but_first_bases", "time_deltas"]
            },
            "mean": {
                "function": "mean",
                "args": ["rate"],
                "range": [0, 0]
            }
        }
    },
    "fluxomeCorrelation": {
        "data": {
            "FLUX_UNITS": {
                "import": "models.ecoli.analysis.single.centralCarbonMetabolismScatter.FLUX_UNITS"
            },
            "sim_reaction_ids": {
                "table": "FBAResults",
                "attribute": "reactionIDs"
            },
            "sim_fluxes_unprocessed": {
                "table": "FBAResults",
                "column": "reactionFluxes",
                "units": {
                    "import": "models.ecoli.analysis.single.centralCarbonMetabolismScatter.FLUX_UNITS"
                }
            },
            "toya_fluxes_unprocessed": {
                "cPickle": "validation_data",
                "dotted_name": "reactionFlux.toya2010fluxes",
                "dict_key": "reactionFlux"
            },
            "toya_reaction_ids": {
                "cPickle": "validation_data",
                "dotted_name": "reactionFlux.toya2010fluxes",
                "dict_key": "reactionID"
            },
            "cell_mass": {
                "table": "Mass",
                "column": "cellMass",
                "units": "fg"
            },
            "dry_mass": {
                "table": "Mass",
                "column": "dryMass",
                "units": "fg"
            },
            "cell_density": {
                "constant": 1100,
                "units": "g/L"
            }
        },
        "operations": {
            "toya_fluxes_adjusted": {
                "function": "adjust_toya_data",
                "args": ["toya_fluxes_unprocessed", "cell_mass", "dry_mass", "cell_density"]
            },
            "filter_ids": {
                "function": "fluxome_common_ids",
                "args": ["toya_reaction_ids", "sim_reaction_ids"]
            },
            "sim_fluxes": {
                "function": "process_simulated_fluxes",
                "args": ["filter_ids", "sim_reaction_ids", "sim_fluxes_unprocessed"]
            },
            "toya_fluxes": {
                "function": "process_toya_data",
                "args": ["filter_ids", "toya_reaction_ids", "toya_fluxes_adjusted"]
            },
            "sim_fluxes_unitless": {
                "function": "strip_units",
                "args": ["sim_fluxes", "FLUX_UNITS"]
            },
            "toya_fluxes_unitless": {
                "function": "strip_units",
                "args": ["toya_fluxes", "FLUX_UNITS"]
            },
            "correlation": {
                "function": "pearson_correlation",
                "args": ["sim_fluxes_unitless", "toya_fluxes_unitless"],
                "range": [0, 0]
            }
        }
    },
    "limiting_metabolites": {
        "data": {
            "metabolite_counts": {
                "table": "EnzymeKinetics",
                "column": "metaboliteCountsFinal"
            },
            "metabolite_names": {
                "table": "EnzymeKinetics",
                "attribute": "metaboliteNames"
            },
            "WINDOW": {
                "import": "models.ecoli.analysis.multigen.limitedMetabolites.WINDOW"
            },
            "REF_COL_INDEX": {
                "constant": 1
            }
        },
        "operations": {
            "normalized_counts": {
                "function": "normalize_to_column",
                "args": ["metabolite_counts", "REF_COL_INDEX"]
            },
            "limiting_metabolites": {
                "function": "find_limiting_metabolites",
                "args": ["normalized_counts", "metabolite_names", "WINDOW"],
                "expected_set": []
            },
            "num_limiting_metabolites": {
                "function": "len",
                "args": ["limiting_metabolites"],
                "range": [0, 0]
            }
        }
    }
}
